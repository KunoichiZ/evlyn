{
  "name": "shyna",
  "version": "0.0.1",
  "description": "Monitor and Reviver",
  "main": "src/shyna.js",
  "scripts": {
    "deps:install": "yarn install",
    "deps:install-production": "yarn install --production",
    "deps:update-unix": "rm -rf node_modules && rm yarn.lock && yarn install",
    "deps:update-windows": "rd -r -Force node_modules && rm yarn.lock && yarn install",
    "deps:update-unix-production": "rm -rf node_modules && rm yarn.lock && yarn install --production",
    "deps:update-windows-production": "rd -r -Force node_modules && rm yarn.lock && yarn install --production",
    "dev:unix": "DEV=true node --experimental-modules src/Shyna.js",
    "dev:windows": "$env:DEV=\"true\"; node --experimental-modules src/Shyna.js",
    "lint": "eslint --fix src",
    "pm2:delete": "pm2 delete Shyna",
    "pm2:logs": "pm2 logs Shyna --lines=200",
    "pm2:restart": "pm2 restart Shyna",
    "pm2:start-harmony": "pm2 start start.js --node-args=\"--experimental-modules --harmony\" --name=\"Shyna\"",
    "pm2:start": "pm2 start start.js --node-args=\"--experimental-modules\" --name=\"Shyna\"",
    "pm2:stop": "pm2 stop Shyna",
    "start": "node --experimental-modules src/shyna.mjs",
    "test": "eslint --ext .mjs src"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kyranet/Shyna.git"
  },
  "keywords": [
    "discord.js",
    "klasa",
    "reviver",
    "monitor"
  ],
  "author": "kyraNET",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/kyranet/Shyna/issues"
  },
  "homepage": "https://github.com/kyranet/Shyna#readme",
  "dependencies": {
    "discord.js": "github:discordjs/discord.js",
    "klasa": "github:dirigeants/klasa#esm",
    "klasa-dashboard-hooks": "github:dirigeants/klasa-dashboard-hooks",
    "veza": "0.3.2"
  },
  "devDependencies": {
    "@types/node": "^10.10.1",
    "eslint": "^5.6.0",
    "eslint-config-klasa": "github:dirigeants/klasa-lint",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3"
  }
}
